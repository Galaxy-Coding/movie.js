{
    "source": {
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|out)"
    },

    "plugins": ["plugins/markdown"],

    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "assets/template/docdash",
        "theme_opts": {
            "theme": "dark"
        }
    }
}